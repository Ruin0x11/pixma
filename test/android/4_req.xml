<?xml version="1.0" encoding="utf-8" ?>
<cmd xmlns:ivec="http://www.canon.com/ns/cmd/2008/07/common/">
  <ivec:contents>
    <ivec:operation>SetConfiguration</ivec:operation>
    <ivec:param_set servicetype="print">
      <ivec:jobID>00000002</ivec:jobID>
      <ivec:papersize>na_index-4x6_4x6in</ivec:papersize>
      <ivec:papertype>custom-media-type-canon-15</ivec:papertype>
      <ivec:borderlessprint>ON</ivec:borderlessprint>
      <ivec:printcolormode>color</ivec:printcolormode>
      <ivec:printcolormode_intent>correct</ivec:printcolormode_intent>
      <ivec:duplexprint>OFF</ivec:duplexprint>
      <ivec:printquality>auto</ivec:printquality>
      <ivec:inputbin>auto</ivec:inputbin>
    </ivec:param_set>
  </ivec:contents>
</cmd>
